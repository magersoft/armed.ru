                //select.quickview__select
                    option.quickview__select-option Москва
                    option.quickview__select-option Санкт-Петербург
                    option.quickview__select-option Караганда
                    option.quickview__select-option Сыктывкар
                    option.quickview__select-option Балашиха
            .quickview-item_buy__delivery-popup
                .delivery-car
                    span.delivery-car_icon
                    p.delivery-car_text
                        | Доставка:&nbsp;
                        span.changed_delivery
                            | 800 &#8381;
                .delivery-self
                    span.delivery-self_icon
                    p.delivery-self_text
                        | Самовывоз:&nbsp;
                    a(href="#" data-id="PickList").delivery-self_link.modalLink
                        | 5 пунктов
