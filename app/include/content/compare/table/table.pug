                                    th.table-head__th-another
                                        p
                                            | Дальность действия ПДУ
                                tr.TrHover(data-id="ten")
                                    th.table-head__th-another
                                        p
                                            | Срок службы



                        .scroll-row.scroll-row-lol
                            .table__item-body
                                include ../CompareData/CompareData
                                each item in CompareData
                                    .table__wrapper-body
                                        table.table-prod
                                            tr
                                                td.td-first
                                                    span.td-del
                                                    .td-first__header
                                                        p #{item.name}
                                                    img(src=item.image alt=item.name)
                                                    .td-first__cost-group
                                                        .td-first__cost-group--new
                                                            p
                                                                | #{item.cost}
                                                                span
                                                                    | &nbsp;&#8381;
                                                        .td-first__cost-group--old
                                                            p
                                                                | #{item.oldcost}
